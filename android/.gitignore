# ============================================
# Android-specific .gitignore
# ============================================
# This complements the root .gitignore
# Android-specific patterns that should be ignored

# Built application files
*.apk
*.ap_
*.aab
*.aar

# Files for the ART/Dalvik VM
*.dex

# Java class files
*.class

# Generated files
bin/
gen/
out/
release/
debug/

# Gradle files
.gradle/
build/
!gradle-wrapper.jar
!gradle-wrapper.properties
.gradletasknamecache

# Local configuration file (sdk path, etc)
local.properties

# Proguard folder
proguard/
app/proguard/

# Log Files
*.log

# Android Studio Navigation editor temp files
.navigation/

# Android Studio captures folder
captures/

# IntelliJ / Android Studio
*.iml
.idea/
.idea/workspace.xml
.idea/tasks.xml
.idea/gradle.xml
.idea/assetWizardSettings.xml
.idea/dictionaries
.idea/libraries
.idea/caches
.idea/modules.xml
.idea/navEditor.xml
.idea/shelf/
*.ipr
*.iws

# Keystore files (NEVER COMMIT!)
*.jks
*.keystore
debug.keystore
release.keystore
app/keystore.properties
app/key.properties

# External native build folder
.externalNativeBuild
.cxx/

# Google Services (e.g. APIs or Firebase)
google-services.json
app/google-services.json

# Freeline
freeline.py
freeline/
freeline_project_description.json

# Fastlane
fastlane/report.xml
fastlane/Preview.html
fastlane/screenshots
fastlane/test_output
fastlane/readme.md

# Version control
vcs.xml

# Lint
lint/intermediates/
lint/generated/
lint/outputs/
lint/tmp/
lint/reports/

# Android Profiling
*.hprof

# Capacitor generated files
app/src/main/assets/public/
app/src/main/assets/capacitor.config.json
app/src/main/assets/capacitor.plugins.json
app/src/main/res/xml/config.xml

# Capacitor/Cordova plugins (generated build outputs only)
capacitor-cordova-android-plugins/build/
capacitor-cordova-android-plugins/.gradle/

# OS files
.DS_Store
Thumbs.db

# Test outputs
app/build/test-results/
app/build/reports/tests/
app/build/test-results.xml

# Gitkeep files (placeholder files for empty directories)
**/.gitkeep
.gitkeep
